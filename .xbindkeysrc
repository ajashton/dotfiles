"pamixer --increase 5 && paplay /usr/share/sounds/freedesktop/stereo/audio-volume-change.oga"
  XF86AudioRaiseVolume

"pamixer --decrease 5 && paplay /usr/share/sounds/freedesktop/stereo/audio-volume-change.oga"
  XF86AudioLowerVolume

"xbacklight -dec 5 -time 0"
  XF86MonBrightnessDown

"xbacklight -inc 5 -time 0"
  XF86MonBrightnessUp
